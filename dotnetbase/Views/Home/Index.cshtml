@{
    ViewData["Title"] = "Home Page";
    var categoryMenu = ViewData["CategoryMenu"] as List<CategoryMenu>;
}

<div class="row">
    <div class="card">
        <img class="background-profile"  src="https://i.imgur.com/K7A78We.jpg" alt="Card image cap">
        <div class="card-body little-profile text-center">
            <div class="pro-img">
                <img class="img-fluid rounded-circle" src="https://i.imgur.com/8RKXAIV.jpg" alt="user">
            </div>
            <h3 class="m-b-0">Brad Macullam</h3>
            <p>Web Designer &amp; Developer</p> <a href="javascript:void(0)" class="m-t-10 waves-effect waves-dark btn btn-primary btn-md btn-rounded" data-abc="true">Follow</a>
            <div class="row text-center m-t-20">
                @foreach (var @item
                    in @categoryMenu)
                {
                    <div class="col-lg-4 col-md-4 m-t-20">
                        <h3 class="m-b-0 font-light">@item.Title.</h3><small>@item.Description</small>
                    </div>
                }

                @*<div class="col-lg-4 col-md-4 m-t-20">
                    <h3 class="m-b-0 font-light">434K</h3><small>Followers</small>
                </div>
                <div class="col-lg-4 col-md-4 m-t-20">
                    <h3 class="m-b-0 font-light">5454</h3><small>Following</small>*@
                @*</div>*@
            </div>
        </div>
    </div>
</div>